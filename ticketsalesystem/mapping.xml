<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="at.fhj.swd.CategoryEntity">
        <table name="category" schema="public" catalog="haring"/>
        <attributes>
            <id name="description">
                <column name="description"/>
            </id>
            <basic name="price">
                <column name="price"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.ContingentEntity">
        <table name="contingent" schema="public" catalog="haring"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="quantity">
                <column name="quantity"/>
            </basic>
            <basic name="fkMatchDate">
                <column name="fk_match_date"/>
            </basic>
            <basic name="fkMatchTime">
                <column name="fk_match_time"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.CustomerEntity">
        <table name="customer" schema="public" catalog="haring"/>
        <attributes>
            <id name="customerNumber">
                <column name="customer_number"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.MatchEntity">
        <table name="match" schema="public" catalog="haring"/>
        <id-class class="at.fhj.swd.MatchEntityPK"/>
        <attributes>
            <id name="pkDate">
                <column name="pk_date"/>
            </id>
            <id name="pkTime">
                <column name="pk_time"/>
            </id>
            <basic name="stadium">
                <column name="stadium"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.PlaceEntity">
        <table name="place" schema="public" catalog="haring"/>
        <id-class class="at.fhj.swd.PlaceEntityPK"/>
        <attributes>
            <id name="placeNumber">
                <column name="place_number"/>
            </id>
            <id name="placeRow">
                <column name="place_row"/>
            </id>
            <id name="placeSection">
                <column name="place_section"/>
            </id>
            <basic name="fkCategoryDescription">
                <column name="fk_category_description"/>
            </basic>
            <basic name="fkStadionId">
                <column name="fk_stadion_id"/>
            </basic>
            <basic name="fkTicketNumber">
                <column name="fk_ticket_number"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.StadiumEntity">
        <table name="stadium" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="totalCapacity">
                <column name="total_capacity"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.TeamEntity">
        <table name="team" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.TeamMatchEntity">
        <table name="team_match" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
            <basic name="fkTeamAId">
                <column name="fk_team_a_id"/>
            </basic>
            <basic name="fkTeamBId">
                <column name="fk_team_b_id"/>
            </basic>
            <basic name="fkMatchDate">
                <column name="fk_match_date"/>
            </basic>
            <basic name="fkMatchTime">
                <column name="fk_match_time"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.TicketEntity">
        <table name="ticket" schema="public" catalog="haring"/>
        <attributes>
            <id name="ticketNumber">
                <column name="ticket_number"/>
            </id>
            <basic name="date">
                <column name="date"/>
            </basic>
            <basic name="fkCustomerNumber">
                <column name="fk_customer_number"/>
            </basic>
            <basic name="fkContingentId">
                <column name="fk_contingent_id"/>
            </basic>
            <basic name="fkPlaceNumber">
                <column name="fk_place_number"/>
            </basic>
            <basic name="fkPlaceRow">
                <column name="fk_place_row"/>
            </basic>
            <basic name="fkPlaceSection">
                <column name="fk_place_section"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.WithTrackEntity">
        <table name="with_track" schema="public" catalog="haring"/>
        <attributes>
            <basic name="fkStadiumId">
                <column name="fk_stadium_id"/>
            </basic>
            <basic name="numberTracks">
                <column name="number_tracks"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.WithoutTrackEntity">
        <table name="without_track" schema="public" catalog="haring"/>
        <attributes>
            <basic name="fkStadiumId">
                <column name="fk_stadium_id"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
