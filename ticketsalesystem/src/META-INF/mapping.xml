<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="at.fhj.swd.Category">
        <table name="category" schema="public" catalog="haring"/>
        <attributes>
            <id name="description">
                <column name="description"/>
            </id>
            <basic name="price">
                <column name="price"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Contingent">
        <table name="contingent" schema="public" catalog="haring"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="quantity">
                <column name="quantity"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Customer">
        <table name="customer" schema="public" catalog="haring"/>
        <attributes>
            <id name="customerNumber">
                <column name="customer_number"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Match">
        <table name="match" schema="public" catalog="haring"/>
        <id-class class="at.fhj.swd.MatchPK"/>
        <attributes>
            <id name="pkDate">
                <column name="pk_date"/>
            </id>
            <id name="pkTime">
                <column name="pk_time"/>
            </id>
            <basic name="stadium">
                <column name="stadium"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Place">
        <table name="place" schema="public" catalog="haring"/>
        <id-class class="at.fhj.swd.PlacePK"/>
        <attributes>
            <id name="placeNumber">
                <column name="place_number"/>
            </id>
            <id name="placeRow">
                <column name="place_row"/>
            </id>
            <id name="placeSection">
                <column name="place_section"/>
            </id>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Stadium">
        <table name="stadium" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="totalCapacity">
                <column name="total_capacity"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Team">
        <table name="team" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.TeamMatch">
        <table name="team_match" schema="public" catalog="haring"/>
        <attributes>
            <id name="pkId">
                <column name="pk_id"/>
            </id>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.Ticket">
        <table name="ticket" schema="public" catalog="haring"/>
        <attributes>
            <id name="ticketNumber">
                <column name="ticket_number"/>
            </id>
            <basic name="date">
                <column name="date"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.WithTrack">
        <table name="with_track" schema="public" catalog="haring"/>
        <attributes>
            <id name="id"/>
            <basic name="fkStadiumId">
                <column name="fk_stadium_id"/>
            </basic>
            <basic name="numberTracks">
                <column name="number_tracks"/>
            </basic>
        </attributes>
    </entity>
    <entity class="at.fhj.swd.WithoutTrack">
        <table name="without_track" schema="public" catalog="haring"/>
        <attributes>
            <id name="id"/>
            <basic name="fkStadiumId">
                <column name="fk_stadium_id"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
